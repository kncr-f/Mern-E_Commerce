    "dependencies": {
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-async-handler": "^1.2.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.2.4",
        "morgan": "^1.10.0",
        "multer": "^1.4.4"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
